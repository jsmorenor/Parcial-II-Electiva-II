              <properties>
                <property
                  dataType="System.String"
                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT O.SalesOrderID AS Origin_Id,
       O.OrderDate,
       O.DueDate AS RequiredDate,
       O.ShipDate,
       O.Freight,	   
       A.City,
       A.CountryRegion,
       A.PostalCode,
       A.AddressLine1 AS AddressLine, 
       PoO.Id_Client AS Customer_Id,
       'A' AS Origin
FROM SalesLT.SalesOrderHeader O
INNER JOIN SalesLT.Address A ON A.AddressID = O.ShipToAddressID
INNER JOIN SalesLT.Customer C ON C.CustomerID = O.CustomerID
INNER JOIN ParcialOut.dbo.Clients PoO ON PoO.Origin_Id = C.CustomerID
WHERE PoO.Origin = 'A'</property>
