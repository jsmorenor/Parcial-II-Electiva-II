                  dataType="System.String"
                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
ProductCategoryID AS ChildProductCategoryID, Name, 'A' AS Origin FROM AdventureWorksLT2014.SalesLT.ProductCategory WHERE ParentProductCategoryID IS NULL
UNION
SELECT
Child.ProductCategoryID, CONCAT(Parent.Name, ' - ', Child.Name) AS Child, 'A' AS Origin
FROM AdventureWorksLT2014.SalesLT.ProductCategory Parent
INNER JOIN AdventureWorksLT2014.SalesLT.ProductCategory Child
ON Child.ParentProductCategoryID = Parent.ProductCategoryID
WHERE Child.ParentProductCategoryID IS NOT NULL;</property>
