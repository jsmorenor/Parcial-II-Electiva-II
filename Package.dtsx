              <properties>
                <property
                  dataType="System.String"
                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
  CONCAT(A.AddressLine1, CASE WHEN A.AddressLine2 IS NOT NULL THEN CONCAT(' - ', A.AddressLine2) ELSE '' END) AS Address
  , A.City
  , A.StateProvince AS Region
  , A.CountryRegion AS Country
  , A.PostalCode
  , CA.CustomerID
FROM AdventureWorksLT2014.SalesLT.Address A
INNER JOIN AdventureWorksLT2014.SalesLT.CustomerAddress CA ON A.AddressID = CA.AddressID</property>
